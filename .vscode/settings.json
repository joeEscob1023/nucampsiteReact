{
  "workbench.colorCustomizations": {
    "terminal.foreground": "#08cbe9",
    "terminal.background": "#0f0f0f",
    "terminalCursor.background": "#7af541",
    "terminalCursor.foreground": "#ff00bf",
    "terminal.selectionBackground": "#ff6dd352"
  },

  "editor.tokenColorCustomizations": {
    "functions": "#63e1e6",
    "keywords": "#0ceee3",
    "variables": "#87e976",
    "strings": "#85eee7",
    "numbers": "#C26AEA",
    "types": "#e98cd5",
    "textMateRules": [
      {
        "scope": "variable.other",
        "settings": {
          "foreground": "#e277ee",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#70F9BE"
        }
      },
      {
        "scope": "alias",
        "settings": {
          "foreground": "#1b95e7"
        }
      }
    ],
    "comments": {
      "foreground": "#bb3793",
      "fontStyle": "italic"
    }
  }
}
